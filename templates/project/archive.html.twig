{% extends "base.html.twig" %}

{% block pageTitle %}{% trans %}Project Archive{% endtrans %}{% endblock %}

{% block body %}
<section class="content">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">{% trans %}Project Archive{% endtrans %}</h3>
                </div>
                <div class="card-body">
                    <table class="table table-sm table-bordered table-striped table-hover project--archive--table-projects">
                        <thead>
                        <tr>
                            <th>{% trans %}ID{% endtrans %}</th>
                            <th>{% trans %}Name{% endtrans %}</th>
                            <th>{% trans %}Customer / Company{% endtrans %}</th>
                            <th>{% trans %}Budget{% endtrans %}</th>
                            <th>{% trans %}Progress{% endtrans %}</th>
                            <th>{% trans %}Actions{% endtrans %}</th>
                        </tr>
                        </thead>
                        <tbody>
                        </tbody>
                        <tfoot>
                        <tr>
                            <th>{% trans %}ID{% endtrans %}</th>
                            <th>{% trans %}Name{% endtrans %}</th>
                            <th>{% trans %}Customer / Company{% endtrans %}</th>
                            <th>{% trans %}Budget{% endtrans %}</th>
                            <th>{% trans %}Progress{% endtrans %}</th>
                            <th>{% trans %}Actions{% endtrans %}</th>
                        </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
{% endblock %}
